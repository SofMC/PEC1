    private void check4_1MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_check4_1MouseClicked
        // TODO add your handling code here:
        if(check4_1.isSelected()){
        comboServFisico.setEnabled(true);
        comboServFisico.removeAllItems();
        check4_2.setEnabled(false);
      Iterator it= arr.getFisico().iterator();
      while(it.hasNext()){
       Servidor fis = (Servidor)it.next();
       comboServFisico.addItem(fis.getSerie());
      }}
        else{
        comboServFisico.setEnabled(false);
        check4_2.setEnabled(true);}
    }//GEN-LAST:event_check4_1MouseClicked

