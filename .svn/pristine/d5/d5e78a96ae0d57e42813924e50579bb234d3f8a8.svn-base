    private void bot1_GuarMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_bot1_GuarMouseClicked
Servidor ser=new Servidor();
Virtual vir= new Virtual();
//---------------------------AGREGANDO SERVIDOR VIRTUAL-------------------------- 
        if(check_Virtual.isSelected()){    
        vir.setSerie(txt3_serie1.getText());
        vir.setHostname(txt3_host1.getText());
        vir.setIp(txt3_ip1.getText());
        vir.setCantMaemoria(txt3_mem1.getText());
        vir.setDataCenter(txt3_Data6.getText());
        vir.setTecnologia(txt3_Tec6.getText());
        vir.setTamdisc(txt3_tam1.getText());
        vir.setCantCPU(txt3_CPU1.getText());
        vir.setOs((String) combo3_2.getSelectedItem()); 
        vir.setVirtualizacion((String) combo4_8.getSelectedItem());
         if(check3_32_1.isSelected()){
           vir.setArquitectura(check3_32_1.getText());}
         else{ vir.setArquitectura(Check3_64_2.getText());}
        if(arr.CargarVirt(vir)==true)
            JOptionPane.showMessageDialog(null,"Servidor Agregado");
        else
          JOptionPane.showMessageDialog(null,"Servidor no puedo ser gragado, si dejo espacios en blancos debe llenarlo o su Serie y hostname ya estan siendo utilizados"); 
        }
  //--------------------------AGREGANDO SERVIDOR FISICO -------------------------     
        else{
            ser.setSerie(txt3_serie1.getText());
        ser.setHostname(txt3_host1.getText());
        ser.setIp(txt3_ip1.getText());
        ser.setCantMaemoria(txt3_mem1.getText());
        ser.setTamdisc(txt3_tam1.getText());
        ser.setOs((String) combo3_2.getSelectedItem()); 
        ser.setCantCPU(txt3_CPU1.getText());
        if(check3_32_1.isSelected()){
           ser.setArquitectura(check3_32_1.getText());
        }else{
           ser.setArquitectura(Check3_64_2.getText());
        }
        if(arr.CargarFisico(ser)==true)
        JOptionPane.showMessageDialog(null,"Servidor Agregado");
        else
          JOptionPane.showMessageDialog(null,"Servidor no puedo ser gragado, si dejo espacios en blancos debe llenarlo o su Serie y hostname ya estan siendo utilizados");  
        }
        //--------------------LIMPIANDO------------------
        txt3_serie1.setText("");
        txt3_host1.setText("");
        txt3_ip1.setText("");
        txt3_mem1.setText("");
        txt3_tam1.setText("");
        combo3_2.setSelectedIndex(0);
        check3_32_1.setEnabled(true);
        Check3_64_2.setEnabled(true);
        check3_32_1.setSelected(false);
        Check3_64_2.setSelected(false);
        txt3_Data6.setText("");
        txt3_Tec6.setText("");
        combo4_8.setSelectedIndex(0);
        txt3_CPU1.setText("");
        combo2_1.setSelectedIndex(0);
    }//GEN-LAST:event_bot1_GuarMouseClicked

