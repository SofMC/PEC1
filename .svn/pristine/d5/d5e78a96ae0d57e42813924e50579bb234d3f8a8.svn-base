        }
       if(check4_2.isSelected()){
            V1.setSerie((String)comboServVirtual.getSelectedItem());
       if( arr.EliminarVirt(V1)==true){
      JOptionPane.showMessageDialog(null,"Servidor Virtual Eliminado"); }
      comboServVirtual.removeAllItems();
      Iterator it= arr.GetVirtual().iterator();
      while(it.hasNext()){
       Virtual vir = (Virtual)it.next();
       comboServVirtual.addItem(vir.getSerie());}
       check4_2.setSelected(false);
       comboServVirtual.setEnabled(false);
       check4_1.setEnabled(true);
       }  
       
       
    }//GEN-LAST:event_jButton1MouseClicked

